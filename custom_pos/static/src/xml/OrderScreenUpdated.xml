        <xpath expr="//div[hasclass('pos-receipt-contact')]" position="inside">
           
                <div style="font-weight:bold;">Mobile Number:
                    <t t-esc='receipt.phoneNumber'/>
                </div>
            
        </xpath>
    </t>
</templates>